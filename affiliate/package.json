{
	"name": "affiliate-product-integrations",
	"version": "0.0.1",
	"description": "Affiliate Product Card Block",
	"author": "Organic Team",
	"scripts": {
		"build": "wp-scripts build blocks/productCard/src/index.js --output-path=blocks/productCard/build/ && wp-scripts build blocks/productCarousel/src/index.js --output-path=blocks/productCarousel/build/",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"start": "wp-scripts start",
		"start:product-card": "wp-scripts start blocks/productCard/src/index.js --output-path=blocks/productCard/build/",
		"build:product-card": "wp-scripts build blocks/productCard/src/index.js --output-path=blocks/productCard/build/",
		"start:product-carousel": "wp-scripts start blocks/productCarousel/src/index.js --output-path=blocks/productCarousel/build/",
		"build:product-carousel": "wp-scripts build blocks/productCarousel/src/index.js --output-path=blocks/productCarousel/build/",
		"start:hero": "wp-scripts start blocks/hero/src/index.js --output-path=blocks/hero/build/",
		"build:hero": "wp-scripts build blocks/hero/src/index.js --output-path=blocks/hero/build/"
	},
	"devDependencies": {
		"@babel/core": "7.18.0",
		"@babel/eslint-parser": "7.17.0",
		"@babel/preset-react": "7.17.12",
		"eslint": "8.15.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react": "7.30.0"
	},
	"dependencies": {
		"@wordpress/block-editor": "^10.5.0",
		"@wordpress/blocks": "^11.21.0",
		"@wordpress/components": "^22.1.0",
		"@wordpress/element": "^4.18.0",
		"@wordpress/rich-text": "^5.20.0",
		"@wordpress/scripts": "23.0.0",
		"lodash": "^4.17.21",
		"prop-types": "15.8.1"
	}
}
